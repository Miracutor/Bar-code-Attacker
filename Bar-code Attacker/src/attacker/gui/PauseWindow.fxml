<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="80.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="attacker.gui.PauseWindow_Controller">
   <center>
      <AnchorPane prefHeight="100.0" prefWidth="300.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="resumeButton" layoutX="39.0" layoutY="28.0" mnemonicParsing="false" onAction="#resume" text="Resume" />
            <Button fx:id="returnButton" layoutX="176.0" layoutY="28.0" mnemonicParsing="false" onAction="#return_title" text="Return to Title" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
